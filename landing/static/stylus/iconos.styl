@font-face 
	font-family 'iconic'
	src url('../fuentes/iconic.eot')
	src url('../fuentes/iconic.eot?#iefix') format('embedded-opentype'), url('../fuentes/iconic.woff') format('woff'), url('../fuentes/iconic.ttf') format('truetype'), url('../fuentes/iconic.svg#iconic') format('svg')
	font-weight normal
	font-style normal


[class*="icon-"]:before 
	font-family 'iconic'
	content attr(data-icon)
	speak none
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale


.icon-user:before 
	content "\e000"

.icon-at:before 
	content "\e001"

.icon-check:before 
	content "\e002"

